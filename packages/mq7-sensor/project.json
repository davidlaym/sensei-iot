{
  "root": "packages/mq7-sensor",
  "sourceRoot": "packages/mq7-sensor/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/mq7-sensor",
        "main": "packages/mq7-sensor/src/index.ts",
        "tsConfig": "packages/mq7-sensor/tsconfig.lib.json",
        "assets": ["packages/mq7-sensor/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/mq7-sensor/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/mq7-sensor"],
      "options": {
        "jestConfig": "packages/mq7-sensor/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
